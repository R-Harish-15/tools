from machine import Pin. UART
uart = UART(0, 9600)
ledPin(16, Pin. OUT)
while True:
if uart.any() > 0:
  data=uart.read()print(data)
  if "A" in data:
    led.value(1) 
    print(LED on in') 
    uart.write('LED on \n')
  elif "B" in data: 
      led.value(0) 
      print('LED off \n') 
      uart.write(LED off \n')
